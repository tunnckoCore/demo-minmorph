{
  "private": true,
  "name": "demo-minmorph",
  "version": "0.0.0-semantic-release",
  "description": "wip - Phenomenal DOM diffing",
  "repository": "tunnckoCore/demo-minmorph",
  "author": "Charlike Mike Reagent (https://i.am.charlike.online)",
  "nspId": "sasa",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rm -rf .cache dist node_modules/.cache",
    "bundle": "parcel src/app/index.html --port 3000",
    "cmd": "yarn clean && yarn bundle",
    "predev": "yarn cmd",
    "dev": "chokidar 'src/**' -c 'yarn cmd'"
  },
  "license": "Apache-2.0",
  "licenseStart": 2018,
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "bel": "5.1.6",
    "domdiff": "0.3.6",
    "nanomorph": "5.1.3",
    "parcel-bundler": "1.5.1"
  },
  "devDependencies": {
    "asyncro": "3.0.0",
    "chokidar-cli": "1.2.0",
    "eslint": "4.18.1",
    "eslint-config-tunnckocore": "0.3.0",
    "hela": "2.0.8",
    "hela-config-tunnckocore": "2.2.44",
    "math-random-seed": "1.0.0",
    "rollup": "0.55.5",
    "tape": "4.9.0"
  },
  "keywords": [
    "automation"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "p-map-series",
        "execa",
        "hela",
        "mri"
      ],
      "highlight": "execa"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "hela",
      "semantic-release"
    ]
  }
}

{
  "private": true,
  "name": "demo-minmorph",
  "version": "0.0.0-semantic-release",
  "description": "wip - Phenomenal DOM diffing",
  "repository": "tunnckoCore/demo-minmorph",
  "author": "Charlike Mike Reagent (https://i.am.charlike.online)",
  "nspId": "sasa",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rm -rf .cache dist node_modules/.cache",
    "budo:tests": "budo src/app/test.js --live --port 5000",
    "bundle": "parcel src/app/index.html --port 5000",
    "cmd": "yarn clean && yarn bundle",
    "predev": "yarn cmd",
    "dev": "chokidar 'src/**' -c 'yarn cmd'"
  },
  "license": "Apache-2.0",
  "licenseStart": 2018,
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "bel": "6.0.0",
    "budo": "^11.3.0",
    "domdiff": "0.4.2",
    "nanohtml": "^1.2.4",
    "nanomorph": "^6.0.0-beta4",
    "parcel-bundler": "^1.9.2",
    "serve": "^9.1.0"
  },
  "devDependencies": {
    "asyncro": "3.0.0",
    "chokidar-cli": "1.2.0",
    "eslint": "4.19.1",
    "eslint-config-tunnckocore": "0.3.0",
    "hela": "2.0.10",
    "hela-config-tunnckocore": "2.2.44",
    "math-random-seed": "1.0.0",
    "rollup": "0.61.1",
    "tape": "4.9.1"
  },
  "keywords": [
    "automation"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "p-map-series",
        "execa",
        "hela",
        "mri"
      ],
      "highlight": "execa"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "hela",
      "semantic-release"
    ]
  }
}
